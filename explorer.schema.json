                "then": {
                    "properties": {
                        "metrics": {
                            "type": "array",
                            "allOf": [
                                {
                                    "contains": {
                                        "$ref": "#/definitions/exploration_info"
                                    }
                                }
                            ]
                        }
                    }
                }
            },
            {
                "if": {
                    "properties": {
                        "columnType": {
                            "const": "bool"
                        },
                        "status": {
                            "const": "Complete"
                        }
                    }
                },
                "then": {
                    "properties": {
                        "metrics": {
                            "type": "array",
                            "allOf": [
                                {
                                    "contains": {
