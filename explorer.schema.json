                        "status": {
                            "const": "Complete"
                        }
                    }
                },
                "then": {
                    "properties": {
                        "metrics": {
                            "type": "array",
                            "allOf": [
                                {
                                    "contains": {
                                        "$ref": "#/definitions/exploration_info"
                                    }
                                }
                            ]
                        }
                    }
                }
            },
            {
                "if": {
                    "properties": {
                        "columnType": {
                            "const": "text"
                        },
                        "status": {
                            "const": "Complete"
                        }
                    }
                },
                "then": {
                    "oneOf": [
                        {
                            "description": "metrics for categorical text columns",
                            "properties": {
                                "metrics": {
                                    "type": "array",
                                    "allOf": [
                                        {
                                            "contains": {
                                                "$ref": "#/definitions/exploration_info"
                                            }
                                        },
                                        {
                                            "contains": {
                                                "$ref": "#/definitions/sample_values::string"
                                            }
                                        },
