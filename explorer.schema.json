                        "entropy": {
                            "type": "number"
                        },
                        "mean": {
                            "type": "number"
                        },
                        "mode": {
                            "type": "number"
                        },
                        "quartiles": {
                            "type": "array",
                            "items": {
                                "type": "number"
                            },
                            "minItems": 3,
                            "maxItems": 3
                        },
                        "standardDeviation": {
                            "type": "number"
                        },
                        "variance": {
                            "type": "number"
