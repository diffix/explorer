{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Diffix Explorer",
    "description": "Schema for data produced by Diffix Explorer",
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "format": "uuid"
        },
        "status": {
            "type": "string",
            "enum": [
                "New",
                "Validating",
                "Processing",
                "Complete",
                "Canceled",
                "Error"
            ]
        },
        "versionInfo": {
            "type": "object",
            "properties": {
                "commitRef": {
                    "type": "string"
                },
                "commitHash": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "required": [
                "commitRef",
                "commitHash"
            ]
        },
        "dataSource": {
            "type": "string"
        },
        "table": {
            "type": "string"
        },
        "columns": {
            "type": "array",
            "items": {
                "$ref": "#/definitions/columnMetrics"
            }
        },
        "sampleData": {
            "type": "array",
            "items": {
                "type": "array"
            }
        },
        "errors": {
            "type": "array",
            "items": {
                "type": "string"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "id",
        "status",
        "versionInfo",
        "dataSource",
        "table",
        "columns",
        "sampleData",
        "errors"
