                                                "$ref": "#/definitions/sample_values::integer"
                                            }
                                        },
                                        {
                                            "contains": {
                                                "$ref": "#/definitions/descriptive_stats"
                                            }
                                        },
                                        {
                                            "contains": {
                                                "$ref": "#/definitions/histogram.value_counts"
                                            }
                                        },
                                        {
                                            "contains": {
                                                "$ref": "#/definitions/histogram.suppressed_count"
                                            }
                                        },
                                        {
                                            "contains": {
                                                "$ref": "#/definitions/histogram.suppressed_ratio"
                                            }
                                        },
                                        {
                                            "contains": {
                                                "$ref": "#/definitions/histogram.buckets"
                                            }
                                        },
                                        {
                                            "contains": {
                                                "$ref": "#/definitions/min::integer"
                                            }
                                        },
                                        {
                                            "contains": {
                                                "$ref": "#/definitions/max::integer"
                                            }
                                        },
                                        {
                                            "contains": {
                                                "$ref": "#/definitions/average_estimate"
                                            }
                                        },
                                        {
                                            "contains": {
                                                "$ref": "#/definitions/quartile_estimates"
                                            }
                                        },
                                        {
