                },
                "value": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "value": {
                                "oneOf": [
                                    {
                                        "type": "integer"
                                    },
                                    {
                                        "const": "--OTHER--"
                                    }
                                ]
                            },
                            "count": {
                                "type": "integer"
                            }
                        },
                        "additionalProperties": false,
                        "required": [
                            "value",
                            "count"
                        ]
                    }
                }
            }
        },
        "distinct.values::string": {
            "type": "object",
            "properties": {
                "name": {
                    "const": "distinct.values"
                },
                "value": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "value": {
                                "oneOf": [
