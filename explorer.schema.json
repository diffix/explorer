                            "description": "metrics for non-categorical text columns",
                            "properties": {
                                "metrics": {
                                    "type": "array",
                                    "allOf": [
                                        {
                                            "contains": {
                                                "$ref": "#/definitions/exploration_info"
                                            }
                                        },
                                        {
                                            "contains": {
                                                "$ref": "#/definitions/sample_values::string"
                                            }
                                        },
                                        {
                                            "contains": {
                                                "$ref": "#/definitions/distinct.is_categorical::false"
                                            }
                                        },
                                        {
                                            "contains": {
                                                "$ref": "#/definitions/is_email"
                                            }
                                        },
                                        {
                                            "contains": {
                                                "$ref": "#/definitions/text.length.counts"
                                            }
                                        },
                                        {
                                            "contains": {
                                                "$ref": "#/definitions/text.length.values"
                                            }
                                        }
                                    ]
                                }
                            }
                        }
                    ]
                }
            },
            {
                "if": {
                    "properties": {
                        "columnType": {
                            "const": "timestamp"
                        },
